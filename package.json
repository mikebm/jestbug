{
	"name": "jestbug",
	"version": "1.0.0",
	"description": "Bug reproduction",
	"main": "index.js",
	"scripts": {
		"test": "jest --coverage"
	},
	"author": "Mike McLafferty",
	"license": "ISC",
	"devDependencies": {
		"@types/jest": "22.2.0",
		"jest": "22.4.2",
		"jest-cli": "22.4.2",
		"jest-junit-reporter": "1.1.0",
		"ts-jest": "22.4.1",
		"typescript": "2.7.2"
	},
	"jest": {
		"automock": true,
		"collectCoverage": true,
		"coverageDirectory": "<rootDir>/coverage/jest",
		"coverageReporters": [
			"lcov",
			"json"
		],
		"collectCoverageFrom": [
			"*.js",
			"*.ts",
			"!**/*.json",
			"!**/coverage/**",
			"!**/node_modules/**",
			"!**/vendor/**"
		],
		"transform": {
			"^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
		},
		"moduleFileExtensions": [
			"ts",
			"js",
			"json"
		],
		"testMatch": [
			"<rootDir>/**/__tests__/**/*.ts?(x)"
		],
		"resetMocks": true,
		"resetModules": true,
		"testResultsProcessor": "./node_modules/jest-junit-reporter",
		"unmockedModulePathPatterns": [
			"date-fns",
			"axios",
			"underscore",
			"backbone",
			"jquery",
			"wrappy",
			"depd",
			"uuid",
			"glob",
			"node_modules/express",
			"deep-extend",
			"test/mock",
			"<rootDir>/node_modules/ts-jest/.*"
		]
	},
	"dependencies": {
		"ts-node": "^5.0.1"
	},
	"moduleDirectories": [
		"<rootDir>",
		"node_modules",
		"src"
	]
}
